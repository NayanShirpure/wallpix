
# Example .env file
# For local development, create a .env.local file and add your actual keys there.
# .env.local is ignored by Git.

# Pexels API Key (Rename IMAGE_API_KEY to PEXELS_API_KEY)
# IMPORTANT: This key is used by your backend API routes.
# For deployment (e.g., on Vercel), set this as a secret environment variable in your project settings.
PEXELS_API_KEY="lc7gpWWi2bcrekjM32zdi1s68YDYmEWMeudlsDNNMVEicIIke3G8Iamw"

# Google Generative AI API Key (for Genkit)
# IMPORTANT: This key is used by your server-side Genkit flows.
# For deployment (e.g., on Vercel), set this as a secret environment variable.
# DO NOT prefix with NEXT_PUBLIC_.
GOOGLE_GENAI_API_KEY="YOUR_GOOGLE_GENAI_API_KEY_HERE"

# Base URL for your site - used for sitemap, canonical URLs, OpenGraph, etc.
# For Vercel, this is usually set automatically, but good to have for other environments or local.
NEXT_PUBLIC_SITE_URL="https://wall-pix.netlify.app/"
